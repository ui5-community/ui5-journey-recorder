{
	"name": "com.ui5.journeyrecorder",
	"version": "1.0.0",
	"description": "UI5 Application: com.ui5.journeyrecorder",
	"author": "Adrian Marten",
	"license": "Apache-2.0",
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build self-contained --clean-dest --include-all-dependencies",
		"build:watch": "run-when-changed --watch \"webapp/**\" --exec \"npm run build\"",
		"start": "ui5 serve --port 8080 -o index.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"ts-typecheck": "tsc --noEmit",
		"lint": "eslint webapp",
		"karma": "karma start",
		"karma-ci": "karma start karma-ci.conf.js",
		"karma-ci-cov": "karma start karma-ci-cov.conf.js",
		"test": "npm run lint && npm run karma-ci-cov",
		"wdi5": "wdio run ./webapp/test/e2e/\\wdio.conf.ts",
		"postbuild": "node ./utils/buildSize.js"
	},
	"devDependencies": {
		"@openui5/types": "^1.120.6",
		"@types/chrome": "^0.0.260",
		"@types/jszip": "^3.4.1",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"@ui5/cli": "^3.9.1",
		"@ui5/ts-interface-generator": "^0.8.1",
		"@wdio/cli": "^8.29.7",
		"@wdio/local-runner": "^8.29.7",
		"@wdio/mocha-framework": "^8.29.3",
		"@wdio/spec-reporter": "^8.29.7",
		"eslint": "^8.56.0",
		"glob": "^10.3.10",
		"karma": "^6.4.2",
		"karma-chrome-launcher": "^3.2.0",
		"karma-coverage": "^2.2.1",
		"karma-ui5": "^3.0.4",
		"karma-ui5-transpile": "^3.3.3",
		"rimraf": "^5.0.5",
		"run-when-changed": "^2.1.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"ui5-middleware-livereload": "^3.0.2",
		"ui5-tooling-modules": "^3.3.4",
		"ui5-tooling-transpile": "^3.3.3",
		"wdio-ui5-service": "^2.0.6",
		"zip-a-folder": "^3.1.6"
	},
	"dependencies": {
		"client-zip": "^2.4.4"
	}
}